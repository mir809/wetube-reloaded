extends ../base.pug

include ../mixins/videos-search


block content
    div.search-page
        div.search-page-Interval
            div.search_filter 
                i.fas.fa-filter
                span 필터
            hr
            div.search_videos
                each videoInfo  in videos
                    +video(videoInfo)
                    
                            
                else 
                    li 관련 영상을 찾을 수 없습니다.
            if smallPlayer
                include ../partials/smallPlayer

block scripts
    script(src="/ast/js/videosList.js")
    script(src="/ast/js/smallPlayer.js")